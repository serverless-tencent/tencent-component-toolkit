{
  "name": "tencent-component-toolkit",
  "version": "1.20.10",
  "description": "Tencent component toolkit",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc -p .",
    "test": "jest",
    "test:cdn": "MODULE=cdn jest",
    "test:cls": "MODULE=cls jest",
    "test:triggers": "MODULE=triggers jest",
    "format": "npm run lint && npm run prettier",
    "commitlint": "commitlint -f HEAD@{15}",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "release": "semantic-release",
    "release-local": "node -r dotenv/config node_modules/semantic-release/bin/semantic-release --no-ci --dry-run",
    "check-dependencies": "npx npm-check --skip-unused --update"
  },
  "engines": {
    "node": ">=10.18"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ygsec && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "ygsec && npm run lint:fix && npm run prettier:fix"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "npm run lint:fix",
      "git add ."
    ],
    "**/*.{css,html,js,json,md,yaml,yml}": [
      "npm run prettier:fix",
      "git add ."
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serverless-tencent/tencent-component-toolkit.git"
  },
  "keywords": [
    "tencent-component",
    "tencent-component-toolkit",
    "release",
    "ci"
  ],
  "author": "Tencent Cloud Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/serverless-tencent/tencent-component-toolkit/issues"
  },
  "homepage": "https://github.com/serverless-tencent/tencent-component-toolkit#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.4",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "@ygkit/secure": "^0.0.3",
    "axios": "^0.21.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.2.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.0.8",
    "prettier": "^2.2.1",
    "semantic-release": "^17.0.4",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@tencent-sdk/capi": "^1.1.8",
    "@tencent-sdk/cls": "^0.1.7",
    "@types/lodash": "^4.14.167",
    "@types/node": "^14.14.20",
    "@ygkit/request": "^0.1.8",
    "cos-nodejs-sdk-v5": "^2.8.6",
    "lodash": "^4.17.20",
    "moment": "^2.25.3",
    "tencent-cloud-sdk": "^1.0.5",
    "type-fest": "^0.20.2"
  }
}
